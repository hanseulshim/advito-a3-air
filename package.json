{
  "name": "advito-a3-air",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "start": "cd ./server && yarn start",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "alpha": "vue-cli-service build && aws s3 sync  ./dist s3://alpha.boostlabs/air/ --acl public-read --profile boost",
    "alpha-server": "cd ./server && yarn alpha",
    "beta": "vue-cli-service build && aws s3 sync  ./dist s3://beta.boostlabs/air/ --acl public-read --profile boost",
    "beta-server": "cd ./server && yarn beta",
    "dev": "vue-cli-service build && aws s3 sync  ./dist s3://alpha.boostlabs/air-dev/ --acl public-read --profile boost",
    "dev-server": "cd ./server && yarn dev",
    "prod": "vue-cli-service build && aws s3 sync  ./dist s3://a3-prod1/ --acl public-read --profile advito",
    "prod-server": "cd ./server && yarn prod"
  },
  "dependencies": {
    "apollo-boost": "^0.1.28",
    "babel-polyfill": "^6.26.0",
    "element-ui": "^2.5.4",
    "graphql": "^14.1.1",
    "moment": "^2.24.0",
    "numeral": "^2.0.6",
    "pdfvuer": "^1.4.0",
    "unfetch": "^4.1.0",
    "vue": "^2.5.22",
    "vue-apollo": "^3.0.0-beta.28",
    "vue-js-modal": "^1.3.28",
    "vue-router": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.4.0",
    "@vue/cli-plugin-eslint": "^3.4.0",
    "@vue/cli-service": "^3.4.0",
    "@vue/eslint-config-prettier": "^4.0.1",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "node-sass": "^4.12.0",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.5.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/recommended",
      "@vue/prettier",
      "eslint:recommended",
      "plugin:prettier/recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
